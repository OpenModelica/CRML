Property model TimePeriods_set
	external Period P1;
	external Period P2;
	
	external Event E1;
	external Event E2;
	
	external Real d;
		
	// Constructors
	Periods M is P1 par P2;
	Periods M1 is [ E1, E2 ];
	Periods M2 is ] E1, E2 ];
	Periods M3 is [ E1, E2 [;
	Periods M4 is ] E1, E2 [;
	Periods M5 is [ E1, d ]; 
	Periods M6 is ] E1, d ];
	Periods M7 is [ E1, d [;
	Periods M8 is ] E1, d [;
		
	// Temporal operators
	Periods M9 is M master P1;	// Master period
	Periods M10 is P1 master M;	// Master period
	Periods M11 is M1 master M2;	// Master period
	
end TimePeriods_set;