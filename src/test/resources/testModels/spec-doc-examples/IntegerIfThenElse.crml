model IntegerIfThenElse is {
	Integer n1 is 25;
	Integer n2 is -3;
	Integer n_if_true_then_else is if true then n1 else n2; //value should be n1
	Integer n_if_false_then_else is if true then n1 else n2; //value should be n2
	Integer n_if_undecided_then_else is if true then n1 else n2; //value should be n2
	Integer n_if_undefined_then_else is if true then n1 else n2; //value should be n2
};