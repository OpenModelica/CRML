model Id_no_ext is {
	Operator [ Boolean ]  'id'  Boolean b = b;

	b_true is true;
	b_false is false;
	b_undecided is undecided;
	b_undefined is undefined;
	b_id_true is 'id' b_true; // value should be true
	b_id_false is 'id' b_false; // value should be false
	b_id_undecided is 'id' b_undecided; // value should be undecided
	b_id_undefined is 'id' b_undefined; // value should be undefined
};