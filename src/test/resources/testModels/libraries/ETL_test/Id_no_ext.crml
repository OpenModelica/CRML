model Id_no_ext is {
	Operator [ Boolean ]  'id'  Boolean b = b;

	Boolean b_true is true;
	Boolean b_false is false;
	Boolean b_undecided is undecided;
	Boolean b_undefined is undefined;
	Boolean b_id_true is 'id' b_true; // value should be true
	Boolean b_id_false is 'id' b_false; // value should be false
	Boolean b_id_undecided is 'id' b_undecided; // value should be undecided
	Boolean b_id_undefined is 'id' b_undefined; // value should be undefined
};